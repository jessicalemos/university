<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="uminho.di.aa.User" table="User" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="name" column="Name" type="string" length="255" not-null="false" lazy="false"/>
		<property name="emai" column="Emai" type="string" length="255" not-null="false" lazy="false"/>
		<property name="password" column="Password" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
